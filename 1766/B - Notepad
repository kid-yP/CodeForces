t = int(input())for _ in range(t):    n = int(input())    s = input().strip()     notepad = {}    found = False        for i in range(n - 1):        sub = s[i:i+2]                if sub in notepad:            if i - notepad[sub] >= 2:                found = True                break        else:            notepad[sub] = i        print("YES" if found else "NO")